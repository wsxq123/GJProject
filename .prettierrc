{
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "trailingComma": "es5",
    "bracketSameLine": true,
    "printWidth": 80,
    "linebreak-style": [true, "LF"],
    "endOfLine":"auto",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "html"
        }
      }
    ]
  }